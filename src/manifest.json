{
    "manifest_version": 3,
    "name": "Hegarty Maths Cheats",
    "version": "1.0.0",
    "description": "A Chrome extension for hegarty Maths. By Tyler Pearson",
    "update_url": "https://clients2.google.com/service/update2/crx",
    "action": {
        "default_title": "Hegarty Maths Cheats",
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "icons/favicon.png",
            "19": "icons/favicon.png",
            "32": "icons/favicon.png",
            "38": "icons/favicon.png"
        }
    },
    "icons": {
        "128": "icons/favicon.png",
        "16": "icons/favicon.png",
        "48": "icons/favicon.png"
    },
    "permissions": [
        "webRequest",
        "tabs",
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "*://hegartymaths.com/",
        "*://hegartymaths.com/*",
        "*://*.hegartymaths.com/",
        "*://*.hegartymaths.com/*"
    ]
}